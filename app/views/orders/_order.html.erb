<div id="<%= dom_id order %>">
  <p>
    <strong>Title:</strong>
    <%= order.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= order.description %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= order.price %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= order.quantity %>
  </p>

  <p>
    <strong>Email address:</strong>
    <%= order.email_address %>
  </p>

  <p>
    <strong>First Name:</strong>
    <%= order.first_name %>
  </p>

  <p>
    <strong>Last Name:</strong>
    <%= order.last_name %>
  </p>

  <p>
    <strong>address:</strong>
    <%= order.address %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= order.phone_number %>
  </p>

</div>
